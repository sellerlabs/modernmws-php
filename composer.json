{
  "name": "sellerlabs/research-php",
  "description": "PHP client for SellerLabs' Research API",
  "require": {
    "guzzlehttp/guzzle": "4.1.*",
    "sami/sami": "~3.0",
    "nesbot/carbon": "^1.19"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "^4.7"
  },
  "conflict": {
    "sellerlabs/modernmws-php": "*",
    "sellerlabs/nodemws-client": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Eduardo Trujillo",
      "email": "ed@chromabits.com"
    },
    {
      "name": "Benjamin Kovach",
      "email": "benjamin@sellerlabs.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SellerLabs\\Research\\": "src/SellerLabs/Research"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\SellerLabs\\Support\\": "tests/SellerLabs/Support",
      "Tests\\SellerLabs\\Research\\": "tests/SellerLabs/Research"
    }
  }
}
